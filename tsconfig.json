{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths":{
      "@home/*": ["src/app/home/*"],
      "@admin/*": ["src/app/admin/*"],
      "@memberCheckin/*": ["src/app/member-checkin/*"],
      "@surveySelection/*": ["src/app/survey-selection/*"],
      "@models/*": ["src/models/*"],
      "@services/*": ["src/services/*"],
      "@guards/*": ["src/app/guards/*"]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
